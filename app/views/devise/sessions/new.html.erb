           <div class="row">
            <div class="col-md-12">
              <%= t 'Log in' %>
              
                <%= form_for(:user, url: session_path(:user)) do |f| %>
                <div class="form-group">
                  <%= f.label :name, class: "sr-only" %> <br />
                  <%= f.text_field :name, placeholder: "Name", autofocus: true, class: "form-control"%>
                </div>
                <div class="form-group">
                  <%= f.label :password, class: "sr-only", for: "InputPassword" %><br />
                  <%= f.password_field :password, placeholder: "You password", autocomplete: "off", id: "InputPassword2", class: "form-control" %>
                </div>
                <div class="check_box">
                  <label>
                    <%= f.check_box :remember_me %></div>
                    <%= f.label :remember_me %>
                  </label>
                
                <div class="actions">
                  <%= f.submit "Log in", class: "btn btn-primary btn-block" %>
                </div>
                <% end %>

                <% if User.first %>
                <%= render "devise/shared/links" %>
                <%else%>
                <%= link_to "Sign up", new_registration_path(resource_name) %><br />
                <% end%>
              </form>
            </div>

          </div>



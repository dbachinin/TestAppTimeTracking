  <body>

<% if user_signed_in? %>
   <%= render 'layouts/header'%>
<% end %>
  <div class="py-5 text-center">
    <div class="container">
      <div class="row"> </div>
    </div>
  </div>
    <div class="container">
      <div class="conent">
        <% flash.each do |name, msg| %>
            <%= content_tag(:div, msg, class: "alert alert-info") %>
        <% end %>
        <%= tinymce_assets %>
        <%= yield %>
        </div>
    </div>
  </body>
</html>
<script type="text/javascript">
$('document').ready(function() {
  setTimeout(function() {
    $('#flash').slideUp();
  }, 3000);
});
</script>